{
    "compilerOptions": {
      "allowJs": true,                  
      "checkJs": false,                 
      "jsx": "react-jsx",               
      "strict": false,                  
      "noImplicitAny": false,           
      "skipLibCheck": true,             
      "noEmit": true,                   
      "esModuleInterop": true,          
      "resolveJsonModule": true,        
      "forceConsistentCasingInFileNames": true, 
      "module": "esnext",               
      "moduleResolution": "node",       
      "target": "esnext",               
      "isolatedModules": false         
    },
    "include": ["src"],                 
    "exclude": ["node_modules"]         
  }
  